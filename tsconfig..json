{
    "compilerOptions": {
      "target": "es2017",
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "baseUrl": "./src",
      "paths": {
        "@/*": ["*"]
      }
    },
    "include": ["src/**/*"]
  }